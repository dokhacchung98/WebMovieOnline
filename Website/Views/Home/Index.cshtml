@if (moviesSeen != null && moviesSeen.Count() > 0)
{
    <div class="general">
        <h4 class="latest-text w3_latest_text">Phim bạn đã xem</h4>
        <div class="container">
            <a href="#" style="color: #ff8d1b; float: right;">Xem thêm >>></a>
            <br />
